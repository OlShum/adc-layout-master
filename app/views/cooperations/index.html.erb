<p id="notice"><%= notice %></p>

<h1>Cooperations</h1>

<table>
  <thead>
    <tr>
      <th>Company</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cooperations.each do |cooperation| %>
      <tr>
        <td><%= cooperation.company %></td>
        <td><%= cooperation.email %></td>
        <td><%= link_to 'Show', cooperation %></td>
        <td><%= link_to 'Destroy', cooperation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cooperation', new_cooperation_path %>
